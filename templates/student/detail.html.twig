{% extends "layout.html.twig" %}
{% block body %}
    <div class="container text-center">
        <h2>Student ID: {{ student.id }}</h2>
        <h2>Student Name: {{ student.name }}</h2>
        <h2>Student Birthday: {{ student.dob | date('d-m-Y') }}</h2>
        <h2>Student Phone: {{ student.phone }}</h2>
        <h2>Student Address: {{ student.address }}</h2>
    </div>
{% endblock %}
